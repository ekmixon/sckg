<def-group>

  <definition class="compliance" id="service_ip6tables_enabled"
  version="1">
    <metadata>
      <title>Service ip6tables Enabled</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <description>The ip6tables service should be enabled if possible.</description>
    </metadata>
    <criteria comment="package iptables-ipv6 installed and service ip6tables is configured to start" operator="AND">
    <criterion comment="iptables-ipv6 installed" test_ref="test_service_ip6tables_package_iptables-ipv6_installed" />
    <criteria operator="OR" comment="service ip6tables is configured to start">
      <criterion comment="ip6tables runlevel 0" test_ref="test_runlevel0_ip6tables_on" />
      <criterion comment="ip6tables runlevel 1" test_ref="test_runlevel1_ip6tables_on" />
      <criterion comment="ip6tables runlevel 2" test_ref="test_runlevel2_ip6tables_on" />
      <criterion comment="ip6tables runlevel 3" test_ref="test_runlevel3_ip6tables_on" />
      <criterion comment="ip6tables runlevel 4" test_ref="test_runlevel4_ip6tables_on" />
      <criterion comment="ip6tables runlevel 5" test_ref="test_runlevel5_ip6tables_on" />
      <criterion comment="ip6tables runlevel 6" test_ref="test_runlevel6_ip6tables_on" />
    </criteria>
    </criteria>
  </definition>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel0_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel0_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel1_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel1_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel2_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel2_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel3_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel3_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel4_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel4_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel5_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel5_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel6_ip6tables_on"
  version="2">
    <unix:object object_ref="obj_runlevel6_ip6tables_on" />
    <unix:state state_ref="state_service_ip6tables_on" />
  </unix:runlevel_test>
  <unix:runlevel_object id="obj_runlevel0_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">0</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel1_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">1</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel2_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">2</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel3_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">3</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel4_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">4</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel5_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">5</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel6_ip6tables_on" version="1">
    <unix:service_name>ip6tables</unix:service_name>
    <unix:runlevel operation="equals">6</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_state comment="configured to start" id="state_service_ip6tables_on" version="1">
    <unix:start datatype="boolean">true</unix:start>
    <unix:kill datatype="boolean">false</unix:kill>
  </unix:runlevel_state>



  <linux:rpminfo_test check="all" check_existence="all_exist"
  id="test_service_ip6tables_package_iptables-ipv6_installed" version="1"
  comment="package iptables-ipv6 is installed">
    <linux:object object_ref="obj_test_service_ip6tables_package_iptables-ipv6_installed" />
    
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_service_ip6tables_package_iptables-ipv6_installed" version="1">
    <linux:name>iptables-ipv6</linux:name>
  </linux:rpminfo_object>
  

</def-group>