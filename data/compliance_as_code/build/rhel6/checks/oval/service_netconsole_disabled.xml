<def-group>

  

  

  <definition class="compliance" id="service_netconsole_disabled"
  version="1">
    <metadata>
      <title>Service netconsole Disabled</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <description>The netconsole service should be disabled if possible.</description>
    </metadata>
   <criteria comment="package netconsole removed or service netconsole is not configured to start" operator="OR">
    <criterion comment="netconsole removed" test_ref="test_service_netconsole_package_netconsole_removed" />
    <criteria operator="AND" comment="service netconsole is not configured to start">
      <criterion comment="netconsole runlevel 0" test_ref="test_runlevel0_netconsole_off" />
      <criterion comment="netconsole runlevel 1" test_ref="test_runlevel1_netconsole_off" />
      <criterion comment="netconsole runlevel 2" test_ref="test_runlevel2_netconsole_off" />
      <criterion comment="netconsole runlevel 3" test_ref="test_runlevel3_netconsole_off" />
      <criterion comment="netconsole runlevel 4" test_ref="test_runlevel4_netconsole_off" />
      <criterion comment="netconsole runlevel 5" test_ref="test_runlevel5_netconsole_off" />
      <criterion comment="netconsole runlevel 6" test_ref="test_runlevel6_netconsole_off" />
    </criteria>
    </criteria>
  </definition>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel0_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel0_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel1_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel1_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel2_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel2_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel3_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel3_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel4_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel4_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel5_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel5_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel6_netconsole_off"
  version="2">
    <unix:object object_ref="obj_runlevel6_netconsole_off" />
    <unix:state state_ref="state_service_netconsole_off" />
  </unix:runlevel_test>
  <unix:runlevel_object id="obj_runlevel0_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">0</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel1_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">1</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel2_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">2</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel3_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">3</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel4_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">4</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel5_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">5</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel6_netconsole_off" version="1">
    <unix:service_name>netconsole</unix:service_name>
    <unix:runlevel operation="equals">6</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_state comment="not configured to start" id="state_service_netconsole_off" version="1">
    <unix:start datatype="boolean">false</unix:start>
    <unix:kill datatype="boolean">true</unix:kill>
  </unix:runlevel_state>

  




  <linux:rpminfo_test check="all" check_existence="none_exist"
  id="test_service_netconsole_package_netconsole_removed" version="1"
  comment="package netconsole is removed">
    <linux:object object_ref="obj_test_service_netconsole_package_netconsole_removed" />
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_service_netconsole_package_netconsole_removed" version="1">
    <linux:name>netconsole</linux:name>
  </linux:rpminfo_object>


</def-group>