<def-group>

  

  

  <definition class="compliance" id="service_cups_disabled"
  version="1">
    <metadata>
      <title>Service cups Disabled</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <description>The cups service should be disabled if possible.</description>
    </metadata>
   <criteria comment="package cups removed or service cups is not configured to start" operator="OR">
    <criterion comment="cups removed" test_ref="test_service_cups_package_cups_removed" />
    <criteria operator="AND" comment="service cups is not configured to start">
      <criterion comment="cups runlevel 0" test_ref="test_runlevel0_cups_off" />
      <criterion comment="cups runlevel 1" test_ref="test_runlevel1_cups_off" />
      <criterion comment="cups runlevel 2" test_ref="test_runlevel2_cups_off" />
      <criterion comment="cups runlevel 3" test_ref="test_runlevel3_cups_off" />
      <criterion comment="cups runlevel 4" test_ref="test_runlevel4_cups_off" />
      <criterion comment="cups runlevel 5" test_ref="test_runlevel5_cups_off" />
      <criterion comment="cups runlevel 6" test_ref="test_runlevel6_cups_off" />
    </criteria>
    </criteria>
  </definition>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel0_cups_off"
